[webservers]
web-ubuntu ansible_host=hw9-app-ubuntu ansible_ssh_private_key_file=~/.ssh/web-key.pem ansible_os_family=Debian
web-rocky ansible_host=hw9-app-rocky ansible_ssh_private_key_file=~/.ssh/web-key.pem ansible_os_family=RedHat

[dbservers]
db-ubuntu ansible_host=hw9-db-ubuntu ansible_ssh_private_key_file=~/.ssh/web-key.pem ansible_os_family=Debian
db-rocky ansible_host=hw9-db-rocky ansible_ssh_private_key_file=~/.ssh/web-key.pem ansible_os_family=RedHat
